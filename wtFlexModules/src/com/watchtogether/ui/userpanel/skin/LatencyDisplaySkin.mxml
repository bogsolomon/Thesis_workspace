<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.watchtogether.ui.userpanel.LatencyDisplay")]
	</fx:Metadata>
	<s:Rect width="5" height="4" bottom="0">
		<s:fill>
			<s:SolidColor color="{hostComponent.barColor}" alpha="{hostComponent.alphaBar.getItemAt(0) as Number}"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF"/>
		</s:stroke>
	</s:Rect>
	<s:Rect width="5" height="8" bottom="0" x="6">
		<s:fill>
			<s:SolidColor color="{hostComponent.barColor}" alpha="{hostComponent.alphaBar.getItemAt(1) as Number}"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF"/>
		</s:stroke>
	</s:Rect>
	<s:Rect width="5" height="12" bottom="0" x="12">
		<s:fill>
			<s:SolidColor color="{hostComponent.barColor}" alpha="{hostComponent.alphaBar.getItemAt(2) as Number}"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF"/>
		</s:stroke>
	</s:Rect>
	<s:Rect width="5" height="16" bottom="0" x="18">
		<s:fill>
			<s:SolidColor color="{hostComponent.barColor}" alpha="{hostComponent.alphaBar.getItemAt(3) as Number}"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF"/>
		</s:stroke>
	</s:Rect>
	<s:Rect width="5" height="20" bottom="0" x="24">
		<s:fill>
			<s:SolidColor color="{hostComponent.barColor}" alpha="{hostComponent.alphaBar.getItemAt(4) as Number}"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#FFFFFF"/>
		</s:stroke>
	</s:Rect>
</s:Skin>
